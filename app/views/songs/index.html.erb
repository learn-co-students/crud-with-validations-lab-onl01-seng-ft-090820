
<style>
table, th, td {
  border: 4px solid black;
}
</style>

<h1>Song List:</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Artist Name</th>
    <th>Genre</th>
    <th>Released</th>
    <th>Release Year</th>
  </tr>
  <tbody>
  <% @songs.each do |song| %>
  <tr>
    <td><%=  song.title %></td>
    <td><%= song.artist_name %></td>
    <td><%= song.genre %></td>
    <td><%= song.released %></td>
    <td><%= song.release_year %></td>
  </tr>
  <% end %>
  </tbody>
</table>


