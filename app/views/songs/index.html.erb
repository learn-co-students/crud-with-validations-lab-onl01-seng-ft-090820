<h1>Songs</h1>
<table>
    <thead>
        <th>Title:</th>
        <th>Artist:</th>
        <th>Released?</th>
        <th>Release Year:</th>
        <th>Genre:</th>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
            <tr>
                <td><%= link_to song.title, song %></td>
                <td><%= song.artist_name %></td>
                <td><%= song.released %></td>
                <td><%= song.release_year %></td>
                <td><%= song.genre %></td>
            </tr>
        <% end %>
    </tbody>
</table>
<p>
    <%= link_to "New Song", new_song_path %>
</p>