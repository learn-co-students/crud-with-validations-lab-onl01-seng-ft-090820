<h1>Create a Song</h1>
<%= form_for @song do |t| %>
    <%= t.label :title %>
    <%= t.text_field :title %>
    <br>
    <%= t.label :release_year %>
    <%= t.number_field :release_year %>
    <br>
    <%= t.label :artist_name %>
    <%= t.text_field :artist_name %>
    <br>
    <%= t.label :genre %>
    <%= t.text_field :genre %>
    <br>
    <%= t.label :released %>
    <%= t.check_box :released %>
    <br>
    <%= t.submit %>
<% end %>